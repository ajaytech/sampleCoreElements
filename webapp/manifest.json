{
	"_version": "1.5.0",
	"sap.app": {
		"id": "sampleCoreElements",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"optionForUsersDataSrc": {
				"uri": "model/data.json",
				"type": "JSON"
			},
			"globalModelSrc": {
				"uri": "model/dataOverView.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sampleCoreElements.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sampleCoreElements.i18n.i18n"
				}
			},
			"optionForUsers": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "optionForUsersDataSrc"
			},
			"globalModel": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "globalModelSrc"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}],
			"js": [{
				"uri": "util/formatter.js"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sampleCoreElements.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "homepage",
				"target":"home"
			},{
				"pattern": "materialoverview",
				"name": "materialoverview",
				"target":"materialoverview"
			},{
				"pattern": "materialInspection",
				"name": "materialInspection",
				"target":"materialInspection"
			}],
			"targets": {
				"home": {
					"viewType": "XML",
					"viewName": "homepage",
					"viewId":"homepage",
					"viewLevel": 1
				},
				"materialoverview":{
					"viewType": "XML",
					"viewName": "materialOverView",
					"viewId":"materialoverview",
					"viewLevel": 2
				},
				"materialInspection":{
					"viewType": "XML",
					"viewName": "materialInspection",
					"viewId":"materialInspectionID",
					"viewLevel": 2
				}
			}
		}
	}
}